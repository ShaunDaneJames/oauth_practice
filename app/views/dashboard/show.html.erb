<h1>Welcome, <%= current_user.username %></h1>

<h2>Private Repos:</h2>
<ol>
  <% @repo_list.each do |repo| %>
    <% if repo.private %>
    <li>
      <%= repo.name %>
    </li>
    <% end %>
  <% end %>
</ol>


<h2>Public Repos:</h2>
<ol>
<% @repo_list.each do |repo| %>
<% if !repo.private %>
  <li>
    <%= repo.name %>
  </li>
  <% end %>
<% end %>
</ol>
